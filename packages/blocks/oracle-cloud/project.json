{
  "name": "blocks-oracle-cloud",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/blocks/oracle-cloud/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/blocks/oracle-cloud",
        "tsConfig": "packages/blocks/oracle-cloud/tsconfig.lib.json",
        "packageJson": "packages/blocks/oracle-cloud/package.json",
        "main": "packages/blocks/oracle-cloud/src/index.ts",
        "assets": ["packages/blocks/oracle-cloud/*.md"],
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/blocks/oracle-cloud/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
